{
    "name": "kakifrucht/flarum-de-extended",
    "type": "flarum-extension",
    "description": "German language pack for third-party Flarum extensions.",
    "homepage": "https://github.com/Kakifrucht/flarum-de-extended",
    "keywords": ["locale"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Kakifrucht/flarum-de-extended/issues",
        "source": "https://github.com/Kakifrucht/flarum-de-extended",
        "forum": "https://discuss.flarum.org/d/2648"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "authors": [
        {
            "name": "Fabian Prieto Wunderlich",
            "email": "prieto@wunderlich.pw",
            "role": "Translator"
        }
    ],
    "extra": {
        "flarum-extension": {
            "title": "German Extended",
            "category": "language",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#ffffff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "de",
            "title": "Deutsch"
        }
    }
}
