{
    "name": "kakifrucht/flarum-de-extended",
    "type": "flarum-extension",
    "description": "German language pack for third-party Flarum extensions.",
    "homepage": "https://github.com/Kakifrucht/flarum-de-extended",
    "keywords": ["locale"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Kakifrucht/flarum-de-extended/issues",
        "source": "https://github.com/Kakifrucht/flarum-de-extended"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.14"
    },
    "extra": {
        "flarum-extension": {
            "title": "Deutsch Extended",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "de",
            "title": "Deutsch"
        }
    }
}
